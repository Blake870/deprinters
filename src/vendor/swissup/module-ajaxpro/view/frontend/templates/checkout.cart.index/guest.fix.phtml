<script>
require([
    'Magento_Customer/js/customer-data'
], function (customerData) {
    'use strict';

    var cart = customerData.get('cart');

    if (cart().isGuestCheckoutAllowed === false) {
        cart().isGuestCheckoutAllowed = true;
    }
    customerData.invalidate(['ajaxpro-cart']);

    return true;
});
</script>