<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="swissup_gdpr_personal_data_forms_load_before">
        <observer name="swissup_gdpr_register_forms" instance="Swissup\Gdpr\Observer\RegisterPersonalDataForms" />
    </event>
    <event name="swissup_gdpr_personal_data_handlers_load_before">
        <observer name="swissup_gdpr_register_handlers" instance="Swissup\Gdpr\Observer\RegisterPersonalDataHandlers" />
    </event>
</config>
