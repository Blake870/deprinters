<section id="form">
    <h2>Form fields</h2>
    <div class="argento-grid">
        <div class="col-md-5 col-xs-12">
            <form class="form" method="get">
                <fieldset class="fieldset">
                    <div class="field name required">
                        <label class="label" for="name"><span>Name</span></label>
                        <div class="control">
                            <input id="name" title="Name" class="input-text" type="text">
                        </div>
                    </div>
                    <div class="field email required">
                        <label class="label" for="email"><span>Email</span></label>
                        <div class="control">
                            <input id="email" title="Email" class="input-text" type="email">
                        </div>
                    </div>
                    <div class="field gender">
                        <label class="label" for="gender"><span>Gender</span></label>
                        <div class="control">
                            <select id="gender" name="gender" title="Gender">
                                <option value="" selected="selected"> </option>
                                <option value="1">Male</option>
                                <option value="2">Female</option>
                                <option value="3">Not Specified</option>
                            </select>
                        </div>
                    </div>
                    <div class="field required choice">
                        <input type="radio" name="radio" id="radio" class="radio"><label for="radio" class="label"><span>Radio text</span></label>
                    </div>
                    <div class="field required choice">
                        <input type="radio" name="radio" id="radio2" class="radio"><label for="radio2" class="label"><span>Radio text 2</span></label>
                    </div>
                </fieldset>
            </form>
        </div>
        <div class="col-md-5 col-md-push-1 col-xs-12">
            <form class="form" method="get">
                <fieldset class="fieldset">
                    <div class="field required multiple">
                        <label class="label" for="multiple">
                            <span>Multiple choices</span>
                        </label>
                        <div class="control">
                            <select multiple="multiple" class="required-entry" id="multiple" size="3">
                                <option value="1">Option 1</option>
                                <option value="2">Option 2</option>
                            </select>
                        </div>
                    </div>
                    <div class="field comment required">
                        <label class="label" for="comment"><span>What’s on your mind?</span></label>
                        <div class="control">
                            <textarea id="comment" title="What’s on your mind?" class="input-text" cols="5" rows="3"></textarea>
                        </div>
                    </div>
                    <div class="field required choice">
                        <input type="checkbox" id="checkbox" class="checkbox"><label for="checkbox" class="label"><span>Checkbox text</span></label>
                    </div>
                    <div class="field required choice">
                        <input type="checkbox" id="checkbox2" class="checkbox"><label for="checkbox2" class="label"><span>Another text</span></label>
                    </div>
                </fieldset>
            </form>
        </div>
    </div>
</section>
